import java.util.Scanner;
class nfactor{
public static void main(String arg[]){
Scanner input=new Scanner(System.in);
int num=input.nextInt();
int n=input.nextInt();
int x=0;
int a[]=new int[100];
for(int i=1;i<=num;i++)
{
if(num%i==0)
{
a[x]=i;
x++;
}
}
System.out.println(+x);
System.out.println(+a[n-1]);
}
}
